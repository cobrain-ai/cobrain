{
  "name": "@cobrain/calendar",
  "version": "0.1.0",
  "license": "AGPL-3.0",
  "description": "CoBrain calendar integration - Google Calendar and Outlook support",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": ["dist"],
  "scripts": {
